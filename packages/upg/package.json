{
  "name": "@gptlabs/upg",
  "description": "A terminal utility for generating, editing, and executing programs in any language.",
  "keywords": [
    "UPG",
    "GPT-3",
    "GPT Labs"
  ],
  "version": "5.0.0-canary.0",
  "license": "MIT",
  "type": "module",
  "platform": "node",
  "types": "dist/index.d.ts",
  "bin": {
    "upg": "dist/bin.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.txt"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/bin.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "dev-web": "yarn --cwd ../../../api dev",
    "dev": "tsmodule dev",
    "build": "tsmodule build --standalone \"src/{bin.ts,header.txt,taglines.txt}\"",
    "lint": "eslint --fix",
    "pretest": "yarn build && npm link -f",
    "test": "cross-env NODE_ENV=test concurrently -k -s first \"yarn ava\" \"yarn dev-web\"",
    "update-tests": "cross-env NODE_ENV=test concurrently -k -s first \"yarn ava -u\" \"yarn dev-web\"",
    "prerelease": "release-it major --preRelease=canary --github.release --ci -VV",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gptlabs/tools.git"
  },
  "dependencies": {
    "clipboardy": "^3.0.0"
  }
}
